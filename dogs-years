// utworzenie zmiennej zawierającej wiek
const myAge = 20;

// utworzenie zmiennej, mówiącej, że pierwsze dwa lata życia psa
// mnożymy przez 10,5
let earlyYears = 2;
earlyYears *= 10,5;

// Dalsze lata życia mnożymy przez 4
let laterYears = myAge -2;
laterYears *= 4;

let myAgeInDogYears = earlyYears + laterYears;

const myName = 'Joanna Krawczyk'.toLowerCase();

console.log(`My name is ${myName}. I am ${myAge} years old in human years which is ${myAgeInDogYears} years old in dog years.`)
